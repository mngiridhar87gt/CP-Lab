#include<stdio.h>
struct student
{
  int age;
  char name[20];
  float marks;
};
int main()
{
    struct student s1 = {18, "Priya", 40};
    printf("age: %d\n", s1.age);
    printf("name: %s\n", s1.name);
    printf("marks: %f\n\n", s1.marks);

    struct student s2 = {18, "Priyanka", 60};
    printf("age: %d\n", s2.age);
    printf("name: %s\n", s2.name);
    printf("marks: %f\n\n", s2.marks);
    int avg;
    avg = (s1.marks+s2.marks)/2;
    printf("average=%d\n", avg);
    return 0;
}