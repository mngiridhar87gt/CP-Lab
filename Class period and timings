#include <stdio.h>
int main()
{
    int ch;
    printf("enter the period");
    scanf("%d", &ch);
    switch (ch)
    {

case 1:
    printf("subject:Maths\n");
    printf("time-7:15 to 8:15");
    break;

    case 2:
    printf("subject:C Programming\n");
    printf("time-8:15 to 9:15");
    break;

    case 3:
    printf("subject:Physics\n");
    printf("time-9:15 to 10:15");
    break;

    default:
        printf("invalid");
    }
    return 0;
}
