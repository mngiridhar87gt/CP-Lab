
#include<stdio.h>
int main()
{
    int age;
    char USN[20];
    char name[20];
    printf("Enter your Name: ");
    scanf("%s",&name);
    printf("Enter your class number: ");
    scanf("%s",&USN);
    printf("Enter your age: ");
    scanf("%d",&age);


    printf("Your Name: %s\n",name);
    printf("Your Class Number: %s\n",USN);
    printf("Your Age: %d\n",age);

    return 0;
}
