#include<stdio.h>
#include<string.h>
int main(){
    char str1[30]="hello";
    char str2[30]="word";
    printf("length of string s1:%lu\n", strlen(str1));
    strcat(str1, str2);
    printf("after concatenation:%s\n", str1);
    strcpy(str2,str1);
    printf("copied string:%s\n", str2);
    printf("comparision result:%d\n", strcmp(str1,str2));
    return 0;
}