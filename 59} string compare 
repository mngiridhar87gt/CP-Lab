#include<stdio.h>
#include<string.h>
int main(){
    char s1[20];
    char s2[20];
    printf("enter the name");
    scanf("%s", s1);
    printf("enter the name");
    scanf("%s", s2);
    int result = strcmp (s1, s2);
    if
        (result==0)
    printf("stings are equal");

    else if
        (result>0)
    printf("s1 is greater than s2");

    else
    printf("s2 is greater than s1");
}
